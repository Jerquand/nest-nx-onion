{
  "version": 1,
  "projects": {
    "onion": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/onion",
      "sourceRoot": "apps/onion/src",
      "prefix": "onion-first",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/onion",
            "index": "apps/onion/src/index.html",
            "main": "apps/onion/src/main.ts",
            "polyfills": "apps/onion/src/polyfills.ts",
            "tsConfig": "apps/onion/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/onion/src/favicon.ico", "apps/onion/src/assets"],
            "styles": ["apps/onion/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/onion/src/environments/environment.ts",
                  "with": "apps/onion/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "onion:build",
            "proxyConfig": "apps/onion/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "onion:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "onion:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/onion/tsconfig.app.json",
              "apps/onion/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/onion/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/onion/jest.config.js",
            "tsConfig": "apps/onion/tsconfig.spec.json",
            "setupFile": "apps/onion/src/test-setup.ts"
          }
        }
      }
    },
    "onion-e2e": {
      "root": "apps/onion-e2e",
      "sourceRoot": "apps/onion-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/onion-e2e/cypress.json",
            "tsConfig": "apps/onion-e2e/tsconfig.e2e.json",
            "devServerTarget": "onion:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "onion:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/onion-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/onion-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "backend-user-ui-api-rest": {
      "root": "libs/backend/user/ui/api/rest",
      "sourceRoot": "libs/backend/user/ui/api/rest/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/backend/user/ui/api/rest/tsconfig.lib.json",
              "libs/backend/user/ui/api/rest/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/backend/user/ui/api/rest/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/backend/user/ui/api/rest/jest.config.js",
            "tsConfig": "libs/backend/user/ui/api/rest/tsconfig.spec.json"
          }
        }
      }
    },
    "backend-user-core-application": {
      "root": "libs/backend/user/core/application",
      "sourceRoot": "libs/backend/user/core/application/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/backend/user/core/application/tsconfig.lib.json",
              "libs/backend/user/core/application/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/backend/user/core/application/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/backend/user/core/application/jest.config.js",
            "tsConfig": "libs/backend/user/core/application/tsconfig.spec.json"
          }
        }
      }
    },
    "backend-user-core-domain": {
      "root": "libs/backend/user/core/domain",
      "sourceRoot": "libs/backend/user/core/domain/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/backend/user/core/domain/tsconfig.lib.json",
              "libs/backend/user/core/domain/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/backend/user/core/domain/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/backend/user/core/domain/jest.config.js",
            "tsConfig": "libs/backend/user/core/domain/tsconfig.spec.json"
          }
        }
      }
    },
    "backend-user-shell": {
      "root": "libs/backend/user/shell",
      "sourceRoot": "libs/backend/user/shell/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/backend/user/shell/tsconfig.lib.json",
              "libs/backend/user/shell/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/backend/user/shell/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/backend/user/shell/jest.config.js",
            "tsConfig": "libs/backend/user/shell/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "onion"
}
